<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="simplePlaceable" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
		<name>
				<en>Imbiss Booth Decoration</en>
				<de>Imbissbude Dekoration</de>
		</name>

		<functions>
		<function>
        <en>Imbiss booth for french fries and sausages as decorative object.</en>
        <de>Imbissbude für Pommes Frites und Würstchen als Deko-Objekt.</de>
		</function>
		</functions>

		<image>store_marketImbiss.dds</image>
        <price>200</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>gardenSheds</category>
        <brush>
            <type>placeable</type>
            <category>decoration</category>
            <tab>uncategorized</tab>
        </brush>
        <canBeSold>true</canBeSold>
        <showInStore>true</showInStore>
        <vertexBufferMemoryUsage>801536</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>127488</indexBufferMemoryUsage>
        <textureMemoryUsage>7012352</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <filename>marketImbiss.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <testAreas>
            <testArea startNode="testArea1Start" endNode="testArea1End" />
        </testAreas>
        <sounds>
            <place template="medImp" />
        </sounds>
    </placement>

    <clearAreas>
        <clearArea startNode="clearArea1Start" widthNode="clearArea1Width" heightNode="clearArea1Height"/>
    </clearAreas>

    <leveling requireLeveling="true" maxSmoothDistance="1" maxSlope="75" maxEdgeAngle="30">
        <levelAreas>
            <levelArea startNode="levelArea1Start" widthNode="levelArea1Width" heightNode="levelArea1Height"/>
        </levelAreas>
    </leveling>

    <indoorAreas>
        <indoorArea startNode="indoorArea1Start" widthNode="indoorArea1Width" heightNode="indoorArea1Height" />
    </indoorAreas>

    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
    </ai>


    <animatedObjects>
        <animatedObject saveId="door" >
            <animation duration="1.5">
                <part node="0>1|0">
                    <keyFrame time="0.0" rotation="0 0 0"/>
                    <keyFrame time="1.0" rotation="0 175 0"/>
                </part>
            </animation>
            <controls triggerNode="0>1|1" posAction="ACTIVATE_HANDTOOL" posText="action_openGate" negText="action_closeGate" />
            <sounds>
                <moving file="$data/sounds/prefab/gate/backDoorLoop.wav" loops="0" linkNode="0>1|0" volume="0.8" radius="25" innerRadius="3" fadeOut="0.25"/>
                <posEnd file="$data/sounds/prefab/gate/gateOpened.wav" linkNode="0>1|0" volume="0.8" radius="25" innerRadius="3" />
                <negEnd file="$data/sounds/prefab/gate/gateClosed.wav" linkNode="0>1|0" volume="0.8" radius="25" innerRadius="3" />
            </sounds>
        </animatedObject>

        <animatedObject saveId="grill" >
            <animation duration="30">
                <part node="1|4">
                    <keyFrame time="0.0" visibility="false"/>
                    <keyFrame time="0.01" visibility="true"/>
                </part>
            </animation>
            <controls triggerNode="1|5" posAction="ACTIVATE_HANDTOOL" posText="l10nGrill" negText="l10nGrillAgain" />
            <sounds>
                <moving file="frizzle.ogg" loops="0" linkNode="1|4" volume="0.8" radius="25" innerRadius="3" fadeOut="0.25"/>
            </sounds>
        </animatedObject>


  </animatedObjects>


    <i3dMappings>
        <i3dMapping id="testArea1Start" node="0>3|1" />
        <i3dMapping id="testArea1End" node="0>3|1|0" />
        <i3dMapping id="clearArea1Start" node="0>3|0" />
        <i3dMapping id="clearArea1Width" node="0>3|0|0" />
        <i3dMapping id="clearArea1Height" node="0>3|0|1" />
        <i3dMapping id="levelArea1Start" node="0>3|0" />
        <i3dMapping id="levelArea1Width" node="0>3|0|0" />
        <i3dMapping id="levelArea1Height" node="0>3|0|1" />
        <i3dMapping id="indoorArea1Start" node="0>3|2" />
        <i3dMapping id="indoorArea1Width" node="0>3|2|0" />
        <i3dMapping id="indoorArea1Height" node="0>3|2|1" />
        <i3dMapping id="tipOcclusionUpdateArea1Start" node="0>3|3" />
        <i3dMapping id="tipOcclusionUpdateArea1End" node="0>3|3|0" />
    </i3dMappings>
</placeable>
