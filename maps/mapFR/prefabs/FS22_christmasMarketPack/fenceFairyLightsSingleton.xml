<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="fence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>fence singleton</name>
        <functions>
            <function>$l10n_function_decoration</function>
        </functions>
        <image>store_fairyLights.dds</image>
        <price>0</price>
        <dailyUpkeep>0</dailyUpkeep>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>fences</category>
        <canBeSold>true</canBeSold>
        <showInStore>false</showInStore>

        <brush>
            <type>none</type>
        </brush>
        <audioMemoryUsage>0</audioMemoryUsage>
        <vertexBufferMemoryUsage>96000</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>18944</indexBufferMemoryUsage>
        <textureMemoryUsage>1769472</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <filename>fenceFairyLights.i3d</filename>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true">
        <sounds>
            <place template="fenceWoodImp" />
            <destroy template="destroyFence" />
        </sounds>
    </placement>

    <fence maxVerticalAngle="45" maxVerticalGateAngle="2">
        <poles node="0>" /> <!-- contains list of children each is a variant -->
        <panels node="1>" length="4" /> <!-- contains list of children each is a variant -->
    </fence>

    <clearAreas/>
    <leveling/>
    <indoorAreas/>
    <ai><updateAreas/></ai>
</placeable>
